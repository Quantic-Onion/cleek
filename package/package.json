{
  "name": "cleek",
  "version": "1.7.3",
  "description": "Complete UX Vue library",
  "repository": "",
  "author": "Quantic Onion",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "./dist/cleek.umd.js",
  "module": "./dist/cleek.es.js",
  "exports": {
    ".": {
      "import": "./dist/cleek.es.js",
      "require": "./dist/cleek.umd.js"
    }
  },
  "unpkg": "./dist/cleek.umd.js",
  "jsdelivr": "./dist/cleek.umd.js",
  "scripts": {
    "dev": "vitepress dev docs",
    "build": "vite build",
    "build:docs": "vitepress build docs",
    "serve": "vitepress serve docs",
    "analyze": "vite build --config analyze.config.js",
    "patch-publish": "npm run build && npm version patch && npm publish",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src",
    "format": "prettier .  --write"
  },
  "peerDependencies": {
    "vue": "^3.1.5"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^1.2.5",
    "@vue/compiler-sfc": "^3.1.5",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-vue": "^7.14.0",
    "prettier": "^2.3.2",
    "rollup-plugin-visualizer": "^5.5.2",
    "vite": "^2.4.3",
    "vitepress": "^0.21.4"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-brands-svg-icons": "^5.15.4",
    "@fortawesome/free-regular-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/vue-fontawesome": "^3.0.0-5",
    "mitt": "^3.0.0",
    "pug": "^3.0.2",
    "quantic-methods": "^1.0.1",
    "stylus": "^0.56.0",
    "tabbable": "^5.2.0",
    "tippy.js": "^6.3.1",
    "vue": "^3.2.25",
    "vue-perfect-scrollbar": "^0.2.1"
  }
}
